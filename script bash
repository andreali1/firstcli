import xml.etree.ElementTree as ET
tree = ET.parse('datos.xml')
root = tree.getroot()
#print(root)

root.tag
root.attrib
for h in root:
  print(h.attrib )

  

for f in root.iter('TABLE'):
  print(f.attrib)
  for g in root.iter('ROW'):
    print(g.attrib)
print("\n holi ")
 

 
